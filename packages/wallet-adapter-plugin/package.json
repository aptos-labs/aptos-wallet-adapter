    "eslint": "^8.15.0",
    "eslint-config-adapter": "*",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "aptos": "^1.3.17"
  }
